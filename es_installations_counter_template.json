{
  "template": "installations",
  "settings": {
    "index.codec": "best_compression",
    "refresh_interval": "60s"
  },
  "mappings": {
    "_default_": {
      "dynamic_templates": [
        {
          "strings": {
            "match": "*",
            "match_mapping_type": "string",
            "mapping": {
              "type": "string",
              "doc_values": false,
              "index": "not_analyzed"
            }
          }
        }
      ],
      "_all": {
        "enabled": false
      },
      "properties": {
        "uuid": {
          "type": "string",
          "index": "not_analyzed"
        },
        "@timestamp": {
          "type": "date"
        }
      }
    }
  }
}